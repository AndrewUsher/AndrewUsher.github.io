@keyframes heroText 
    from 
        opacity 0;
        transform: scale(.8)
        transform: translateY(-40vh);
    to 
        opacity 1;
        transform: none;

@keyframes heroButton 
    from 
        opacity: 0;
        transform: translateY(30vh);
    to 
        opacity: 1;
        transform: none;

@keyframes chevron
    from
        transform: translateY(-30px);
        opacity: 0.6;
    to
        opacity: 1;
        transform: none;